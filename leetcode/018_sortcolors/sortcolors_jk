class Solution:
    def sortColors(self, nums: List[int]) -> None:
        """
        Do not return anything, modify nums in-place instead.
        """
        color_dict = {0:0, 1:0, 2:0}
        
        for ele in nums:
            color_dict[ele] += 1
        
        nums[:] = [0] * color_dict[0] + [1] * color_dict[1] + [2] * color_dict[2] 
